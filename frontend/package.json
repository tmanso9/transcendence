{
	"name": "frontend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "vite --port 3001",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"lint": "eslint . --fix --ignore-path .gitignore",
		"precommit": "lint-staged",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.vue": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"dependencies": {
		"@mdi/font": "7.0.96",
		"core-js": "^3.29.0",
		"pinia": "^2.0.0",
		"roboto-fontface": "*",
		"vue": "^3.2.0",
		"vue-cookies": "^1.8.3",
		"vue-router": "^4.0.0",
		"vuetify": "^3.0.0"
	},
	"devDependencies": {
		"@babel/types": "^7.21.4",
		"@types/node": "^18.15.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-typescript": "^11.0.0",
		"eslint": "^8.37.0",
		"eslint-plugin-vue": "^9.3.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"prettier": "^3.1.1",
		"sass": "^1.60.0",
		"typescript": "^5.0.0",
		"unplugin-fonts": "^1.0.3",
		"vite": "^4.2.0",
		"vite-plugin-vuetify": "^1.0.0",
		"vue-tsc": "^1.2.0"
	}
}
